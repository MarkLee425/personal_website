{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "generate": {
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}